{
  "name": "svelte-preprocess-cssmodules",
  "version": "1.3.2",
  "description": "Svelte preprocessor to generate CSS Modules classname on Svelte components",
  "keywords": [
    "svelte",
    "svelte-preprocess",
    "css-modules"
  ],
  "homepage": "https://github.com/micantoine/svelte-preprocess-cssmodules",
  "bugs": {
    "url": "https://github.com/micantoine/svelte-preprocess-cssmodules/issues"
  },
  "author": {
    "name": "micantoine"
  },
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "tsc",
    "dev": "npm run build -- -w",
    "lint": "eslint --ext .ts --fix ./src",
    "format": "prettier --write --loglevel warn ./{src,test}",
    "test": "jest",
    "prepublishOnly": "npm run test && npm run build",
    "install": "npm run build"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/micantoine/svelte-preprocess-cssmodules.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "loader-utils": "^2.0.0",
    "object.fromentries": "^2.0.2",
    "string.prototype.matchall": "^4.0.3"
  },
  "devDependencies": {
    "@types/loader-utils": "^2.0.1",
    "@types/object.fromentries": "^2.0.0",
    "@types/string.prototype.matchall": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "svelte": "^3.22.3",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "svelte": "^3.20.0",
    "typescript": "^4.0.0"
  }
}
